{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "0.1.0",
    "command": "gcc",
    "isShellCommand": true,
    "showOutput": "always",
    "tasks": [
        {
            "taskName": "Debug_runtime",
            "args": [
                "-DDEBUG",
                "-g",
                "-o",
                "Debug/Runtime.out",
                "runtime.c",
                "processeur.c",
                "common_component.c",
                "Syntax_calculate/BNF_C.c",
                "Syntax_calculate/calculate.c"
            ],
            "suppressTaskName": true
        },{
            "taskName": "Build_runtime",
            "args": [
                "-o",
                "Build/Runtime",
                "runtime.c",
                "processeur.c",
                "common_component.c",
                "Syntax_calculate/BNF_C.c",
                "Syntax_calculate/calculate.c"
            ],
            "suppressTaskName": true
        },
        {
            "taskName": "Debug_intepreter",
            "args": [
                "-DDEBUG",
                "-g",
                "-o",
                "Debug/Intepreter.out",
                "mode_calculatrice.c",
                "processeur.c",
                "common_component.c",
                "analyse_lexical.c",
                "Syntax_calculate/BNF_C.c",
                "Syntax_calculate/calculate.c"
            ],
            "suppressTaskName": true
        },{
            "taskName": "Build_intepreter",
            "args": [
                "-o",
                "Build/Intepreter",
                "mode_calculatrice.c",
                "processeur.c",
                "common_component.c",
                "analyse_lexical.c",
                "Syntax_calculate/BNF_C.c",
                "Syntax_calculate/calculate.c"
            ],
            "suppressTaskName": true
        },
        {
            "taskName": "Debug_compiler",
            "args": [
                "-g",
                "-DDEBUG",
                "-o",
                "Debug/Compiler.out",
                "compiler.c",
                "processeur.c",
                "common_component.c",
                "analyse_lexical.c",
                "Syntax_calculate/BNF_C.c",
                "Syntax_calculate/calculate.c"
            ],
            "suppressTaskName": true
        },{
            "taskName": "Build_compiler",
            "args": [
                "-o",
                "Build/Compiler",
                "mode_calculatrice.c",
                "processeur.c",
                "common_component.c",
                "analyse_lexical.c",
                "Syntax_calculate/BNF_C.c",
                "Syntax_calculate/calculate.c"
            ],
            "suppressTaskName": true
        }
    ]
}